{
  "name": "lms",
  "version": "1.0.0",
  "description": "LM Studio SDK",
  "main": "index.js",
  "scripts": {
    "start": "node ./starter.mjs",
    "watch-tsc": "tsc --build tsconfig.build.json --watch",
    "watch-jest": "jest --watch --testMatch **/?(*.)+(test).ts",
    "watch-ui-css": "cd packages/lms-ui && npm run watch-css",
    "watch-sdk-ae": "cd publish/sdk && npm run watch-ae",
    "watch-sdk-webpack": "cd publish/sdk && npm run watch-webpack",
    "watch-cli-package": "cd packages/lms-cli && npm run watch",
    "watch-cli-webpack": "cd publish/cli && npm run watch",
    "watch-lmstudio": "cd publish/lmstudio && npm run watch",
    "clean": "shx rm -rf ./packages/**/dist ./packages/**/tsconfig.tsbuildinfo ./publish/**/dist ./publish/**/ts-out",
    "build-sdk": "cd publish/sdk && npm run build",
    "build-cli": "cd packages/lms-cli && npm run build && cd ../../publish/cli && npm run build",
    "build-lmstudio": "cd publish/lmstudio && npm run build",
    "build-scaffolds-json": "cd scaffolds && node ./buildScaffoldsJSON.js",
    "build": "npm run clean && tsc --build tsconfig.build.json && npm run build-sdk && npm run build-cli && npm run build-lmstudio && npm run build-scaffolds-json",
    "make-cli": "cd packages/lms-cli && npm run build && cd ../../publish/cli && npm run make",
    "make": "npm run clean && tsc --build tsconfig.build.json && npm run build-sdk && npm run make-cli && npm run build-lmstudio && npm run build-scaffolds-json",
    "publish": "npm run build && lerna publish --no-git-tag-version --no-git-reset",
    "upload-scaffolds-manifest": "cd scaffolds && node ./upload.js",
    "manual-testing": "cd ./packages/lms-sdk-manual-testing/dist && node index.js",
    "postinstall": "patch-package && npm run postinstall-cli",
    "postinstall-cli": "cd packages/lms-cli && npm run postinstall",
    "prt": "./postReleaseTag.sh"
  },
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.556.0",
    "@microsoft/api-extractor": "^7.40.1",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "autoprefixer": "^10.4.17",
    "bundle-declarations-webpack-plugin": "^5.0.1",
    "dotenv": "^16.4.5",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "inquirer": "^9.2.14",
    "jest": "^29.7.0",
    "lerna": "^8.1.2",
    "parcel": "^2.11.0",
    "patch-package": "^8.0.0",
    "postcss": "^8.4.35",
    "postcss-cli": "^11.0.0",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "process": "^0.11.10",
    "shx": "^0.3.4",
    "stmux": "^1.8.7",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.90.2",
    "webpack-cli": "^5.1.4"
  },
  "overrides": {
    "node-pty": "npm:@homebridge/node-pty-prebuilt-multiarch"
  },
  "workspaces": [
    "packages/*",
    "publish/*",
    "examples/*",
    "scaffolds/*"
  ],
  "dependencies": {
    "@lmstudio/lms-cli": "^0.2.2",
    "terminal-size": "^4.0.0"
  }
}
